<p><%= @idea.content %></p>
<p><%= @idea.category.title %></p>
<% @idea.images.each do |image| %>
  <%= image_tag(image.image.url(:large)) %>
<% end %>
<p><%= link_to "Back to the box", user_ideas_path %>
<%= link_to "Edit", edit_user_idea_path(@idea) %>
<%= link_to "Delete", user_idea_path(@idea), method: :delete, data: {confirm: "Are you sure?"} %>
